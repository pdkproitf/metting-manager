<p id="notice"><%= notice %></p>
<h1>Listing Profiles</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Level</th>
      <th>Picture</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.level %></td>
        <td><%= profile.picture %></td>
        <td><%= profile.user_id %></td>
        <td><%= link_to 'Show', user_profile_path(profile.user_id, profile) %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(profile.user_id, profile) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
